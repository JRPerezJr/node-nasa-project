{
  "name": "nasa-project-api",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "14.15.1",
    "npm": "6.14.8"
  },
  "description": "NASA Mission Control API",
  "main": "server.js",
  "scripts": {
    "client": "cd ../client && yarn start",
    "build": "cd ../client && yarn build",
    "server": "nodemon src/server.js",
    "start": "node src/server.js",
    "cluster": "pm2 start src/server.js -i max",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "deploy": "concurrently --kill-others-on-fail \"yarn build\" \"yarn start\"",
    "deploy-cluster": "concurrently --kill-others-on-fail \"yarn build\" \"yarn cluster\"",
    "stop": "pm2 stop server",
    "delete": "pm2 delete server",
    "reload": "pm2 reload server",
    "test": "jest --detectOpenHandles",
    "test-watch": "jest --watch"
  },
  "keywords": [],
  "author": "Juan Perez Jr.",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JRPerezJr/node-nasa-project"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.7",
    "morgan": "^1.10.0",
    "pm2": "^4.5.6"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
